<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Published at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.published_at %></td>
        <td><%= link_to 'edit', edit_dashboard_post_path(post) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
